{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "curly": 0,
    "comma-spacing": 0,
    "eqeqeq": [2, "allow-null"],
    "key-spacing": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-extend-native": 0,
    "no-var": 2,
    "new-cap": 0,
    "quotes": 0,
    "semi-spacing": 0,
    "space-unary-ops": 0,
    "space-infix-ops": 0,
    "consistent-return": 0,
    "strict": 0,
    "semi": ["error", "never"],
    "class-methods-use-this": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "arrowFunctions": true
    }
  },
  "env": {
    "es6":true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ]
}
